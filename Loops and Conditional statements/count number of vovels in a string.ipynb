   "source": [
    "user_input = input(\"Enter the string to count vowles = \")\n",
    "count = 0\n",
    "user_input = user_input.lower()\n",
    "for ch in user_input:\n",
    "    if ch.lower() in \"aeiou\":\n",
    "        count += 1\n",
    "print(f\"number of vowels in {user_input} is {count}\")"
   ]
